/* pages/tempkeylist/tempkeylist.wxss */
page {
  height: 100%;
  display: flex;
  flex-direction: column;
  background: rgb(240, 232, 232);
}

.createkey {
  /* width: 100%; */
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background: #fff;
  align-items: center;
  margin-top: 10rpx;
  padding-left: 20rpx;
}

.image {
  width: 88rpx;
  height: 88rpx;
}

.list {
  margin-left: 20rpx;
  margin-right: 20rpx;
  padding-top: 10rpx;
}

.keytypeview {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.keytype {
  font-size: 34rpx;
  color: rgb(114, 109, 109)
}

.item {
  display: flex;
  flex-direction: column;
  background: #fff;
  border-radius: 10rpx;
  align-items: center;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  padding: 10rpx;
}

.line {
  width: 90%;
  height: 1rpx;
  background-color: rgba(65, 36, 36, 0.144);
}

.textback {
  display: flex;
  flex-direction: column;
  width: 200rpx;
  border-radius: 44rpx;
  background-color: #eee;
  align-items: center;
}

.keynum {
  font-size: 40rpx;
  font-weight: bold;
}

.editview {
  width: 90%;
  margin-top: 5rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.date {
  font-size: 34rpx;
  color: rgba(151, 147, 147, 0.849)
}

.edit_text {
  font-size: 34rpx;
  color: rgb(90, 140, 153);
}