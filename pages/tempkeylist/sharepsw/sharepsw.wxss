/* pages/tempkeylist/sharepsw/sharepsw.wxss */

page {
  height: 100%;
  display: flex;
  flex-direction: column;
  background: rgb(240, 232, 232);
}

.item {
  display: flex;
  flex-direction: column;
  background: #fff;
  border-radius: 10rpx;
  align-items: center;
  /* margin-top: 20rpx;
  margin-bottom: 20rpx; */
  margin: 20rpx;
  padding: 10rpx;
}

.image {
  width: 88rpx;
  height: 88rpx;
}

.textback {
  display: flex;
  flex-direction: column;
  width: 200rpx;
  border-radius: 44rpx;
  background-color: #eee;
  align-items: center;
  margin: 30rpx;
}

.text {
  /* padding: 30rpx; */
  font-size: 34rpx;
  color: rgba(151, 147, 147, 0.849)
}

.pswtext {
  margin: 30rpx;
  font-size: 40rpx;
  font-weight: bold;
  margin-top: 80rpx;
}

.button {
  width: 50%;
  background: #428ee2;
  color: rgb(143, 212, 214);
  border-radius: 26px;
  margin-top: 100rpx;
}