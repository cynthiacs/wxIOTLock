/* pages/tempkeylist/addtemppsw/addtemppsw.wxss */
page {
  height: 100%;
}

.all {
  height: 100%;
  display: flex;
  flex-direction: column;
  /* justify-content: space-around; */
  background-color: #f3f3f3
}

.title {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom: 30rpx;
}

.image {
  width: 88rpx;
  height: 88rpx;
  margin-top: 50rpx;
  margin-bottom: 20rpx;
}

.line {
  height: 1rpx;
  width: 100%;
  background-color: #9a9fa5;
  margin-top: 20rpx;
}

.input {
  background-color: white;
  margin: 10rpx;
  padding: 20rpx;
  /* padding-right: 13rpx; */

}

.picker {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  background-color: white;
  padding: 20rpx;
  margin: 10rpx;
}

.moreicon {
  width: 66rpx;
  height: 66rpx;
  transform: rotate(90deg);
}

.text {
  padding: 13px;
  margin: 10rpx;
}

.button {
  width: 50%;
  padding-left: 13px;
  padding-right: 13rpx;
  margin-top: 20rpx;
  background: #428ee2;
  color: rgb(143, 212, 214);
  border-radius: 26px;
}
